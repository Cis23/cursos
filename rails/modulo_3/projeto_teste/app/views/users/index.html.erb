<p id="notice"><%= notice %></p>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Idade</th>
      <th><abbr tittle="Certidão de Pessoal Física">CPF</abbr></th>
      <th>Telefone</th>
      <th>Tipo de usuário</th>
      <th>Tipo teste</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.age %></td>
        <td><%= user.cpf %></td>
        <td><%= user.telephone %></td>
        <td><%= user.type_user.description_name %></td>
        <td><%= link_to t('links.show'), user %></td>
        <td><%= link_to t('links.edit'), edit_user_path(user) %></td>
        <td><%= link_to t('links.destroy'), user, method: :delete, data: { confirm: t('confirmacao.deletar') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Adicionar usuário', new_user_path %>
<%= link_to 'Voltar', root_path %>